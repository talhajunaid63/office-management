<div id="<%= dom_id user %>">
  <p>
    <strong>First name:</strong>
    <%= user.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= user.last_name %>
  </p>

  <p>
    <strong>Job position:</strong>
    <%= user.job_position %>
  </p>

  <p>
    <strong>Salary:</strong>
    <%= user.salary %>
  </p>

  <p>
    <strong>Dob:</strong>
    <%= user.dob %>
  </p>

  <p>
    <strong>Job type:</strong>
    <%= user.job_type %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

</div>
