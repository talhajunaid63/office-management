<p style="color: green"><%= notice %></p>

<h1>Expenses</h1>

<div id="expenses">
  <% @expenses.each do |expense| %>
    <%= render expense %>
    <p>
      <%= link_to "Show this expense", company_expense_path(@company, expense) %>
    </p>
  <% end %>
</div>

<%= link_to "New expense", new_company_expense_path(@company) %>
